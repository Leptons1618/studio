{
  "expo": {
    "name": "ChronoCanvas",
    "slug": "chronocanvas-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4285F4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chronocanvas.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4285F4"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.chronocanvas.mobile",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}
